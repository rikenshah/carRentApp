	<h1>Dashboard</h1>

		<%= link_to "Home", root_path %>
		<br>
		<% if user_signed_in? %>

			<% if current_user.superadmin? %>
				<h3>Welcome Super Admin</h3>
				<br>
				<% if false %>
					<%= link_to("See Super-Admins", "url" , :options) %>
					<br>
					<%= link_to("See Admins", "url" , :options) %>
					<br>
					<%= link_to("See Customers", "url" , :options) %>
				<% end %>
			<% elsif current_user.admin? %>
				<h3>Welcome Admin</h3>
				<% if false %>
				<%= link_to("See Admins", "url" , :options) %>
				<br>
				<%= link_to("See Customers", "url" , :options) %>
				<% end %>
				<br><br>
			
			<% else %>
				<h3>Welcome Customer</h3>
			<% end %>

			<%= link_to "Profile", edit_user_registration_path %>
			<br>
			<%= link_to "See all Cars", cars_path %>
			<br>
			<%= link_to "See all Reservations", reservations_path %>   

			<br><br><br><br>
			<%= link_to('Sign Out', destroy_user_session_path, method: :delete) %>     
		
		<% else %>
		
				<br><br><br>
				<%= link_to('Sign In', new_user_session_path)  %>
				<br>
				<%= link_to("Sign Up", new_user_registration_path) %>
		
		<% end %>