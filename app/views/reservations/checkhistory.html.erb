<br><br><strong>User Checkout history</strong>
  <table class='table table-striped'>
  <thead>
    <tr>
      <th>Car</th>
      <th>Check out Time</th>
      <th>Return Time</th>
      <th>Checked out ?</th>
      <th>Returned ?</th>
      <th>Rent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <% @user_checkout_history.each do |u| %>
		  <tr>
          <td><%= u[1][:car_model] %></td>
          <td><%= u[1][:check_out] %></td>
          <td><%= u[1][:return] %></td>
          <td><%= u[1][:checked_out] %></td>
          <td><%= u[1][:returned]%></td>  
          <td><%= u[1][:rent] %></td>        
        </tr>
      <% end %>
  </tbody>
</table>
  
  <br>
  <strong>Total Rent</strong>
  <%= @total_rent%>